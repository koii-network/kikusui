  "description": "A wrapper utility for the finnie wallet.",
  "main": "./dist/index.js",
  "scripts": {
    "start": "ts-node src/Finnie/index.js",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/**/*.ts'",
