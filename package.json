  "license": "ISC",
  "bugs": {
    "url": "https://github.com/koii-network/Finnie-Wallet-Provider/issues"
  },
  "homepage": "https://github.com/koii-network/Finnie-Wallet-Provider#readme",
  "dependencies": {
    "@typescript-eslint/parser": "^4.32.0",
    "browserify": "^17.0.0",
    "chai-as-promised": "^7.1.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.22",
    "@types/chai-as-promised": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.10.3",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "mocha": "^9.1.2",
    "prettier": "^2.4.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
